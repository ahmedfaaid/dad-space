{
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "build/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "api:dev": {
      "dependsOn": [
        "api#dev"
      ]
    },
    "web:dev": {
      "dependsOn": [
        "web#dev"
      ]
    }
  },
  "baseBranch": "origin/main"
}
